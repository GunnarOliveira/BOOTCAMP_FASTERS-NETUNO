algoritmo "ConversaoCelciusParaFarenheight"
// Função :
// Autor :
// Data : 22/06/2024
// Seção de Declarações 
var
     f: real
     c: real
inicio
// Seção de Comandos 
   escreval("Qual a temperatura daqui?")
   leia(f)
   
   c <- (f - 32) / 1.8
   escreval("No Brasil estaria: ", c:4:1)
fimalgoritmo
